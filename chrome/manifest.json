{
  "manifest_version": 2,
  "name": "DirectLatency",
  "description": "Chrome side extension to measure latency",
  "version": "1.0",
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*"
      ],
      "js": [
        "audscibeacon.js"
      ],
      "run_at": "document_start"
    }
  ]
}